/* Horizontally align boxs */
.box.x-center,
[box].x-center {
  justify-content: center;
}

/* Horizontally align box-cols */
.box-col.x-center,
[box-col].x-center {
  align-items: center;
}

/*************************/

/* Vertically align box children */
.box.y-center,
[box].y-center {
  align-items: center;
}

/* Vertically align box-col children */
.box-col.y-center,
[box-col].y-center {
  justify-content: center;
}

/*************************/

/* Horizontally apply even spacing to box children */
.box.even-space,
[box].even-space {
  justify-content: space-around;
}

/* Vertically apply even spacing to box-col children */
.box-col.even-space,
[box-col].even-space {
  justify-content: space-around;
}

/*************************/

/* Horizontally apply space between box children */
.box.space-inside,
[box].space-inside,
/* Vertically apply space between to box-col children */
.box-col.space-inside,
[box-col].space-inside  {
  justify-content: space-between;
}

/*************************/

/* Horizontally push children to end of box */
.box.align-end,
[box].align-end,
/* Vertically push children to end of box-col */
.box-col.align-end,
[box-col].align-end  {
  justify-content: flex-end;
}

/*
justify-content aligns horizontally, unless
flex-direction: column -- in which case it
aligns vertically.
*/
.jc-around { justify-content: space-around; }
.jc-between { justify-content: space-between; }
.jc-start { justify-content: flex-start; }
.jc-end { justify-content: flex-end; }
.jc-center { justify-content: center; }

/*
align-items aligns a single row of items vertically,
unless flex-direction: column -- in which case it
aligns a single columns of items horizontally.
*/
.ai-center { align-items: center; }
.ai-stretch { align-items: stretch; }
.ai-start { align-items: flex-start; }
.ai-end { align-items: flex-end; }
.ai-baseline { align-items: baseline; }

/*
align-content aligns multiple rows of content vertically,
unless flex-direction: column -- in which case it
aligns multiple rows of content horizontally.
*/
.ac-center { align-content: center; }
.ac-start { align-content: flex-start; }
.ac-end { align-content: flex-end; }
.ac-between { align-content: space-between; }
.ac-stretch { align-content: stretch; }
.as-center { align-self: center; }

/*
align-self aligns a single element within a
row of elements aligned with align-items.
*/
.as-stretch { align-self: stretch; }
.as-start { align-self: flex-start; }
.as-end { align-self: flex-end; }
.as-baseline { align-self: baseline; }

.nowrap { flex-wrap: nowrap; }
.wrap { flex-wrap: wrap; }

